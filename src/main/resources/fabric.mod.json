	"authors": [
		{
			"name": "contaria",
			"contact": {
				"homepage": "https://github.com/KingContaria/"
			}
		}
	],
	"contact": {
		"sources": "https://github.com/KingContaria/SpeedrunAPI",
		"issues": "https://github.com/KingContaria/SpeedrunAPI/issues"
